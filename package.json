{
  "name": "ts-route-kit",
  "version": "2.0.2",
  "description": "A lightweight framework for creating HTTP routes using decorators. It is based on Fastify.",
  "main": "./dist/cjs/index.js",
  "module": "./dist/es/index.js",
  "types": "./dist/es/index.d.ts",
  "exports": {
    "require": "./dist/cjs/index.js",
    "import": "./dist/es/index.js"
  },
  "scripts": {
    "build:es": "tsc --project tsconfig.es.json",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build": "npm run build:es && npm run build:cjs",
    "prepare": "npm run build",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Bastien2203/ts-kit-route"
  },
  "keywords": [
    "decorators",
    "routes",
    "framework",
    "typescript"
  ],
  "author": "Bastien2203",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.9.0",
    "jest": "^29.7.0",
    "reflect-metadata": "^0.2.2",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=21.1.0"
  },
  "files": [
    "dist/**/*",
    "README.md"
  ]
}
